<!DOCTYPE html>
<html lang="en">

<head>
    <title>SignUp</title>
    <style>
    </style>
</head>

<body>
    <%- include('../views/scripts') -%>
        <div class="box">
            <%- include('./components/navbar') -%>
                <h1>my status</h1>
                <div>
                    <div>
                        <h2>versus win rate</h2>
                        <% for (var i=0; i < users.length; i++) { %>
                            <a>
                                Player : <%= users[i].username %> :
                                    Rate : <%= ((users[i].win / (users[i].win + users[i].lose)) * 100).toFixed(0) %>%
                            </a>
                    </div>
                    <% } %>
                </div>
        </div>
</body>

</html>